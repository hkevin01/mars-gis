{
  "name": "mars-gis-frontend",
  "version": "1.0.0",
  "description": "Mars GIS Web Interface - Professional geospatial analysis platform for Mars exploration",
  "private": true,
  "homepage": ".",
  "dependencies": {
    "@tailwindcss/forms": "^0.5.3",
    "@types/ol": "^7.0.0",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@types/react-window": "^1.8.8",
    "@types/three": "^0.178.1",
    "autoprefixer": "^10.4.14",
    "cesium": "^1.132.0",
    "framer-motion": "^10.18.0",
    "lucide-react": "^0.263.1",
    "ol": "^8.2.0",
    "postcss": "^8.4.24",
    "proj4": "^2.19.10",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.0",
    "react-scripts": "5.0.1",
    "react-window": "^1.8.8",
    "tailwindcss": "^3.3.2",
    "three": "^0.179.1",
    "typescript": "^4.9.4",
    "web-vitals": "^3.1.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.4.0",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.4.1",
    "prettier": "^2.8.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "type-check": "tsc --noEmit"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "prettier"
    ],
    "rules": {
      "@typescript-eslint/no-unused-vars": "warn",
      "react-hooks/exhaustive-deps": "warn",
      "no-console": "warn"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "proxy": "http://localhost:8000",
  "keywords": [
    "mars",
    "gis",
    "geospatial",
    "react",
    "typescript",
    "3d",
    "visualization",
    "space",
    "exploration"
  ],
  "author": "MARS-GIS Development Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mars-gis/mars-gis.git"
  },
  "bugs": {
    "url": "https://github.com/mars-gis/mars-gis/issues"
  }
}
