    "@types/d3": "^7.4.0",
    "plotly.js": "^2.18.0",
    "react-plotly.js": "^2.6.0",
    "@types/plotly.js": "^2.12.29",
    "socket.io-client": "^4.6.1",
    "axios": "^1.3.0",
    "date-fns": "^2.29.3",
    "recharts": "^2.4.3",
    "react-query": "^3.39.3",
    "zustand": "^4.3.2",
    "immer": "^9.0.18",
    "react-spring": "^9.6.1",
    "framer-motion": "^8.5.2",
    "react-helmet-async": "^1.3.0",
    "react-error-boundary": "^3.1.4",
    "react-hook-form": "^7.43.0",
    "yup": "^0.32.11",
    "@hookform/resolvers": "^2.9.10",
    "react-helmet-async": "^1.3.0",
    "react-error-boundary": "^3.1.4",
    "web-vitals": "^3.1.1"
  },
  "devDependencies": {
    "@testing-library/react": "^13.4.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.4.0",
    "jest": "^29.4.1",
    "jest-environment-jsdom": "^29.4.1",
    "cypress": "^12.5.1",
    "@cypress/react": "^7.0.2",
    "playwright": "^1.30.0",
    "@playwright/test": "^1.30.0",
    "eslint": "^8.34.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prettier": "^2.8.3",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.0",
    "npm-run-all": "^4.1.5",
    "cross-env": "^7.0.3",
    "msw": "^1.1.0",
    "@storybook/react": "^6.5.16",
    "lighthouse": "^10.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:coverage": "react-scripts test --coverage --watchAll=false",
    "test:unit": "react-scripts test --testPathPattern=__tests__ --watchAll=false",
    "test:unit:coverage": "react-scripts test --testPathPattern=__tests__ --coverage --watchAll=false",
    "test:integration": "react-scripts test --testPathPattern=integration --watchAll=false",
    "test:integration:coverage": "react-scripts test --testPathPattern=integration --coverage --watchAll=false",
    "test:components": "react-scripts test --testPathPattern=components --watchAll=false",
    "test:components:coverage": "react-scripts test --testPathPattern=components --coverage --watchAll=false",
    "test:watch": "react-scripts test",
    "test:a11y": "cypress run --spec cypress/e2e/accessibility.cy.ts",
    "test:performance": "cypress run --spec cypress/e2e/performance.cy.ts",
    "test:coverage:check": "react-scripts test --coverage --watchAll=false --coverageThreshold '{\"global\":{\"branches\":80,\"functions\":80,\"lines\":80,\"statements\":80}}'",
    "test:e2e": "cypress run",
    "test:e2e:open": "cypress open",
    "test:playwright": "playwright test",
    "eject": "react-scripts eject",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "type-check": "tsc --noEmit",
    "analyze": "npm run build && npx bundle-analyzer build/static/js/*.js",
    "prepare": "husky install",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "prettier"
    ],
    "rules": {
      "@typescript-eslint/no-unused-vars": "warn",
      "react-hooks/exhaustive-deps": "warn",
      "no-console": "warn"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "proxy": "http://localhost:8000",
  "keywords": [
    "mars",
    "gis",
    "geospatial",
    "react",
    "typescript",
    "3d",
    "visualization",
    "space",
    "exploration"
  ],
  "author": "MARS-GIS Development Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mars-gis/mars-gis.git"
  },
  "bugs": {
    "url": "https://github.com/mars-gis/mars-gis/issues"
  }
}
