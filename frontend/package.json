    "@types/three": "^0.148.0",
    "@react-three/fiber": "^8.10.0",
    "@react-three/drei": "^9.52.0",
    "cesium": "^1.101.0",
    "resium": "^1.16.1",
    "leaflet": "^1.9.3",
    "react-leaflet": "^4.2.0",
    "@types/leaflet": "^1.9.0",
    "deck.gl": "^8.8.22",
    "@deck.gl/react": "^8.8.22",
    "@deck.gl/core": "^8.8.22",
    "@deck.gl/layers": "^8.8.22",
    "@deck.gl/geo-layers": "^8.8.22",
    "mapbox-gl": "^2.12.0",
    "react-map-gl": "^7.0.20",
